        <entity_class name="GoblinClub" extends="npcClubTemplate"> 
			<property name="Tags" value="entity,male,npc,melee,bandit,DRMid"/>
			<property name="Faction" value="bandits"/> 
                        <property name="PhysicsBody" value="GoblinStandard"/>
                        <property name="LookAtAngle" value="0"/> 
			<property name="Names" value="Grug,Urug,Lumdum,Lorzub,Snaglak,Shamob,Purbag,Orgug" />				
			<property name="Mesh" value="#@modfolder:Resources/Goblinz.unity3d?GoblinClubDark"/>
                        <property name="ItemsOnEnterGame.GameModeSurvival" value="OrcClub"/>
			<property name="ItemsOnEnterGame.GameModeSurvivalSP" value="OrcClub"/>
			<property name="ItemsOnEnterGame.GameModeSurvivalMP" value="OrcClub"/> 
            <effect_group name="Base Effects">
		        <passive_effect name="HealthMax" operation="base_set" value="150"/>
		        <passive_effect name="PhysicalDamageResist" operation="base_set" value="20"/>
	        </effect_group>
		</entity_class>

